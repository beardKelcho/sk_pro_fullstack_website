{
  "name": "skproduction-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:next": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "dev": "next dev -p 3000",
    "dev:clean": "npm run clean:next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --maxWorkers=2",
    "test:ci:stable": "jest --ci --maxWorkers=2 --detectOpenHandles --forceExit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "cypress:run": "start-server-and-test dev http://localhost:3000 \"cypress run\"",
    "analyze": "ANALYZE=true next build",
    "bundle-size": "node scripts/check-bundle-size.js",
    "bundle-size:check": "npm run build && npm run bundle-size",
    "lighthouse": "lighthouse http://localhost:3000 --view",
    "lighthouse:ci": "lighthouse http://localhost:3000 --ci --json --output-path=./lighthouse-report.json",
    "check-production": "ts-node scripts/check-production.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@sentry/nextjs": "^10.35.0",
    "@tanstack/react-query": "^5.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react-grid-layout": "^1.3.6",
    "@types/three": "^0.182.0",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.25.0",
    "gsap": "^3.14.2",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.484.0",
    "mongodb": "^6.15.0",
    "next": "^14.2.35",
    "next-intl": "^4.7.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.12",
    "qr-scanner": "^1.4.2",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^2.2.2",
    "react-hook-form": "^7.54.2",
    "react-is": "^18.3.1",
    "react-quill": "^2.0.0",
    "react-redux": "^9.2.0",
    "react-toastify": "^10.0.4",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.0.2",
    "three": "^0.182.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.28",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.10.2",
    "critters": "^0.0.25",
    "cypress": "^13.6.6",
    "cypress-axe": "^1.6.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lighthouse": "^11.6.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "whatwg-fetch": "^3.6.20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "quill": "^2.0.3"
  }
}
